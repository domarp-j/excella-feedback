<div class="container">

  <h1 class="text-center">Excella Presentations</h1>

    <%= admin_table(current_user) %>

    <% if Participation.where(user_id: current_user.id).length == 0 %>
    <div class="well well-sm text-center">
      <i style="font-size: 18pt;">You are not currently signed up for any presentations.</i>
    </div>
    <% else %>
    <%= general_user_table(
      user: current_user,
      role: :presenter,
      title: "As Presenter",
      feedback_message: "See Feedback"
    ) %>

    <%= general_user_table(
      user: current_user,
      role: :attendee,
      title: "As Attendee",
      feedback_message: "Provide Feedback"
    ) %>
    <% end %>
</div>
<!-- /.container -->
